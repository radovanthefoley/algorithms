class LinkedList {
    Node head;
    
    void addNode(int data) {
        if(head == null) {
            head = new Node(data);
        } else {
            Node next = head.next;
            while (next != null) {
                next = next.next;
            }
            next = new Node(data);
        }
    }
    
    private class Node {
        int data;
        Node next;
        
        Node(int data) {
            this.data = data;
        }
    }
    
    
}